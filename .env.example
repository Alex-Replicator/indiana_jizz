# Indiana Jizz - Настройки окружения
# Скопируйте этот файл в .env и отредактируйте значения

# Основные настройки
COMPOSE_PROJECT_NAME=indiana_jizz
SOURCE_CODE_PATH=../src

# Порты
WEB_PORT=8123
PMA_PORT=8124
MEMCACHED_PORT=11211

# MySQL
MYSQL_ROOT_PASSWORD=root_password
MYSQL_DATABASE=wsw_db
MYSQL_USER=wsw_user
MYSQL_PASSWORD=wsw_password

# PHP
PHP_VERSION=8.1
PHP_MEMORY_LIMIT=512M
PHP_MAX_EXECUTION_TIME=60
PHP_UPLOAD_MAX_FILESIZE=20M
PHP_POST_MAX_SIZE=20M

# Nginx
NGINX_CLIENT_MAX_BODY_SIZE=20M

# Memcached
MEMCACHED_MEMORY=128

# Временная зона
TZ=Europe/Moscow 